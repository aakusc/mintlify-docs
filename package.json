{
  "name": "@bhg/docs-monorepo",
  "version": "1.0.0",
  "description": "Blue Horizons Group Documentation Monorepo - Multiple Mintlify documentation sites",
  "private": true,
  "scripts": {
    "dev": "mintlify dev",
    "dev:sgm": "cd docs/sgm-sparcc && mintlify dev",
    "dev:aicr": "cd docs/aicr-platform && mintlify dev",
    "dev:rally": "cd docs/rally-stack && mintlify dev",
    "dev:towedge": "cd docs/towedge && mintlify dev",
    "dev:psedge": "cd docs/ps-edge && mintlify dev",
    "dev:sfp": "cd docs/sfp && mintlify dev",
    "dev:ispm": "cd docs/intelligentspm && mintlify dev",
    "dev:docai": "cd docs/document-intelligence && mintlify dev",
    "build": "npm run build:all",
    "build:all": "npm run build:sgm && npm run build:aicr && npm run build:rally && npm run build:towedge && npm run build:psedge && npm run build:sfp && npm run build:ispm && npm run build:docai",
    "build:sgm": "cd docs/sgm-sparcc && mintlify build",
    "build:aicr": "cd docs/aicr-platform && mintlify build",
    "build:rally": "cd docs/rally-stack && mintlify build",
    "build:towedge": "cd docs/towedge && mintlify build",
    "build:psedge": "cd docs/ps-edge && mintlify build",
    "build:sfp": "cd docs/sfp && mintlify build",
    "build:ispm": "cd docs/intelligentspm && mintlify build",
    "build:docai": "cd docs/document-intelligence && mintlify build",
    "install:mintlify": "npm install -g mintlify",
    "validate": "npm run validate:all",
    "validate:all": "npm run validate:sgm && npm run validate:aicr && npm run validate:rally && npm run validate:towedge && npm run validate:psedge && npm run validate:sfp && npm run validate:ispm && npm run validate:docai",
    "validate:sgm": "cd docs/sgm-sparcc && mintlify validate",
    "validate:aicr": "cd docs/aicr-platform && mintlify validate",
    "validate:rally": "cd docs/rally-stack && mintlify validate",
    "validate:towedge": "cd docs/towedge && mintlify validate",
    "validate:psedge": "cd docs/ps-edge && mintlify validate",
    "validate:sfp": "cd docs/sfp && mintlify validate",
    "validate:ispm": "cd docs/intelligentspm && mintlify validate",
    "validate:docai": "cd docs/document-intelligence && mintlify validate",
    "list": "echo 'Available documentation sites:' && ls -1 docs/"
  },
  "keywords": [
    "documentation",
    "mintlify",
    "monorepo",
    "blue-horizons-group",
    "sgm",
    "aicr",
    "rally-stack",
    "towedge",
    "ps-edge",
    "sfp",
    "intelligentspm"
  ],
  "author": "Blue Horizons Group",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/AICodeRally/docs-main.git"
  }
}
